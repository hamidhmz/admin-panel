                                placeholder="">
                            <label>Confirm Password</label>
                            <i class="form-group__bar"></i>
                        </div>
                        <button type="button" id="changeBtnPassword" class="btn btn-primary waves-effect"
                            data-toggle="button" aria-pressed="false" autocomplete="off">
